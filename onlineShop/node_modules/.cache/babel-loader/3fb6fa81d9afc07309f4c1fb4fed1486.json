r\n  setup () {\r\n    const { nearbyList, getNearbyList } = useNearbyListEffect()\r\n    const handleBackClick = useHandleBack()\r\n    const handleEditNearby = useEditNearby()\r\n    getNearbyList()\r\n    return { nearbyList, handleBackClick, handleEditNearby }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.nearby {\r\n  background: #fff;\r\n  &__title {\r\n    margin: 0.16rem 0 0.2rem 0;\r\n    font-size: 0.28rem;\r\n    font-weight: normal;\r\n  }\r\n  &__button {\r\n    line-height: 0.28rem;\r\n    margin-bottom: 0.2rem;\r\n    background: #0091ff;\r\n    box-shadow: 0 0.04rem 0.08rem 0 rgba(0, 145, 255, 0.32);\r\n    border-radius: 0.04rem;\r\n    border-radius: 0.04rem;\r\n    color: #fff;\r\n    font-size: 0.16rem;\r\n    text-align: center;\r\n  }\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n  color: #000;\r\n}\r\n\r\n.title__back {\r\n  display: flex;\r\n  line-height: 0.44rem;\r\n  background: #fff;\r\n  font-size: 0.16rem;\r\n  color: #333;\r\n  text-align: center;\r\n  width: 0.3rem;\r\n  font-size: 0.3rem;\r\n  color: #b6b6b6;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}