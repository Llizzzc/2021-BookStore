AI,IAAI,CAAC,UAAL,EAAb,CAfY,CAemB;;AAC/B,MAAA,MAAK,GAAI,MAAK,GAAI,EAAT,GAAc,MAAM,MAApB,GAA6B,MAAtC,CAhBY,CAgBiC;;AAC7C,UAAI,MAAK,GAAI,IAAI,CAAC,UAAL,EAAb,CAjBY,CAiBmB;;AAC/B,MAAA,MAAK,GAAI,MAAK,GAAI,EAAT,GAAc,MAAM,MAApB,GAA6B,MAAtC,CAlBY,CAkBiC;;AAC7C,WAAK,OAAL,aAAkB,IAAlB,cAA0B,KAA1B,cAAmC,GAAnC,cAA0C,IAA1C,cAAkD,MAAlD,cAA4D,MAA5D,cAAsE,OAAO,CAAC,IAAD,CAA7E;AACF;AAxBO,GAPI;AAiCb,EAAA,OAjCa,qBAiCF;AACT,SAAK,WAAL;AACD,GAnCY;AAoCb;AACA,EAAA,aArCa,2BAqCI;AACf,QAAI,KAAK,UAAT,EAAqB;AACnB,MAAA,aAAa,CAAC,KAAK,UAAN,CAAb,CADmB,CACY;AACjC;AACD,GAzCY;AA0Cb,EAAA,KA1Ca,mBA0CJ;AACP,QAAM,QAAO,GAAI,CACf;AACE,MAAA,IAAI,EAAE,IADR;AAEE,MAAA,MAAM,EAAE,OAAO,CAAC,uBAAD;AAFjB,KADe,EAKf;AACE,MAAA,IAAI,EAAE,IADR;AAEE,MAAA,MAAM,EAAE,OAAO,CAAC,uBAAD;AAFjB,KALe,EASf;AACE,MAAA,IAAI,EAAE,IADR;AAEE,MAAA,MAAM,EAAE,OAAO,CAAC,uBAAD;AAFjB,KATe,EAaf;AACE,MAAA,IAAI,EAAE,IADR;AAEE,MAAA,MAAM,EAAE,OAAO,CAAC,uBAAD;AAFjB,KAbe,EAiBf;AACE,MAAA,IAAI,EAAE,IADR;AAEE,MAAA,MAAM,EAAE,OAAO,CAAC,uBAAD;AAFjB,KAjBe,EAqBf;AACE,MAAA,IAAI,EAAE,IADR;AAEE,MAAA,MAAM,EAAE,OAAO,CAAC,uBAAD;AAFjB,KArBe,EAyBf;AACE,MAAA,IAAI,EAAE,IADR;AAEE,MAAA,MAAM,EAAE,OAAO,CAAC,uBAAD;AAFjB,KAzBe,EA6Bf;AACE,MAAA,IAAI,EAAE,IADR;AAEE,MAAA,MAAM,EAAE,OAAO,CAAC,wBAAD;AAFjB,KA7Be,EAiCf;AACE,MAAA,IAAI,EAAE,IADR;AAEE,MAAA,MAAM,EAAE,OAAO,CAAC,uBAAD;AAFjB,KAjCe,EAqCf;AACE,MAAA,IAAI,EAAE,IADR;AAEE,MAAA,MAAM,EAAE,OAAO,CAAC,wBAAD;AAFjB,KArCe,CAAjB;AA0CA,WAAO;AAAE,MAAA,QAAO,EAAP;AAAF,KAAP;AACF;AAtFa,CAAf","sourcesContent":["<template>\n  <div class=\"position\">\n    <span class=\"iconfont position__icon\">&#xe620;</span>\n    时间:{{ nowDate }}\n    <span class=\"iconfont position__bell\">&#xe70b;</span>\n  </div>\n  <div class=\"slogan\">最是书香能致远,腹有诗书气自华</div>\n  <div class=\"banner\">\n    <img\n      class=\"banner__img\"\n      src=\"../../img/others/1.jpg\"\n    />\n  </div>\n  <div class=\"icons\">\n    <div\n      class=\"icons__item\"\n      v-for=\"(item, index) in iconList\"\n      :key=\"index\"\n    >\n      <img\n        class=\"icons__item__img\"\n        :src=\"item.imgUrl\"\n      />\n      <p class=\"icons__item__desc\">{{ item.desc }}</p>\n    </div>\n  </div>\n  <div class=\"gap\"></div>\n</template>\n\n<script>\nexport default {\n  name: 'StaticPart',\n  data () {\n    return {\n      nowDate: '' // 当前日期\n    }\n  },\n  methods: {\n    currentTime () {\n      setInterval(this.formatDate, 500)\n    },\n    formatDate () {\n      // eslint-disable-next-line prefer-const\n      let date = new Date()\n      // eslint-disable-next-line prefer-const\n      let year = date.getFullYear() // 年\n      // eslint-disable-next-line prefer-const\n      let month = date.getMonth() + 1 // 月\n      // eslint-disable-next-line prefer-const\n      let day = date.getDate() // 日\n      // eslint-disable-next-line prefer-const\n      let week = date.getDay() // 星期\n      // eslint-disable-next-line prefer-const\n      let weekArr = ['星期日', '星期一', '星期二', '星期三', '星期四', '星期五', '星期六']\n      let hour = date.getHours() // 时\n      hour = hour < 10 ? '0' + hour : hour // 如果只有一位，则前面补零\n      let minute = date.getMinutes() // 分\n      minute = minute < 10 ? '0' + minute : minute // 如果只有一位，则前面补零\n      let second = date.getSeconds() // 秒\n      second = second < 10 ? '0' + second : second // 如果只有一位，则前面补零\n      this.nowDate = `${year}/${month}/${day} ${hour}:${minute}:${second} ${weekArr[week]}`\n    }\n  },\n  mounted () {\n    this.currentTime()\n  },\n  // 销毁定时器\n  beforeUnmount () {\n    if (this.formatDate) {\n      clearInterval(this.formatDate) // 在Vue实例销毁前，清除时间定时器\n    }\n  },\n  setup () {\n    const iconList = [\n      {\n        desc: '漫画',\n        imgUrl: require('../../img/icons/1.jpg')\n      },\n      {\n        desc: '科幻',\n        imgUrl: require('../../img/icons/2.jpg')\n      },\n      {\n        desc: '美食',\n        imgUrl: require('../../img/icons/3.jpg')\n      },\n      {\n        desc: '教育',\n        imgUrl: require('../../img/icons/4.jpg')\n      },\n      {\n        desc: '儿童',\n        imgUrl: require('../../img/icons/5.jpg')\n      },\n      {\n        desc: '法律',\n        imgUrl: require('../../img/icons/6.jpg')\n      },\n      {\n        desc: '历史',\n        imgUrl: require('../../img/icons/7.jpg')\n      },\n      {\n        desc: '经济',\n        imgUrl: require('../../img/icons/8.jpeg')\n      },\n      {\n        desc: '娱乐',\n        imgUrl: require('../../img/icons/9.jpg')\n      },\n      {\n        desc: '旅游',\n        imgUrl: require('../../img/icons/10.jpg')\n      }\n    ]\n    return { iconList }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.position {\n  position: relative;\n  overflow: hidden;\n  padding: 0.16rem 0.24rem 0.16rem;\n  line-height: 0.22rem;\n  font-size: 0.16rem;\n  white-space: nowrap;\n  text-overflow: ellipsis;\n  .position__icon {\n    position: relative;\n    font-size: 0.2rem;\n    color: red;\n  }\n  .position__bell {\n    position: absolute;\n    right: 0;\n    top: 0.17rem;\n    font-size: 0.2rem;\n    color: gold;\n  }\n}\n\n.slogan {\n  line-height: 0.32rem;\n  background: #f5f5f5;\n  color: #b7b7b7;\n  border-radius: 0.19rem;\n  font-size: 0.14rem;\n  float: right;\n  color: black;\n  font-weight: bold;\n}\n\n.banner {\n  &__img {\n    width: 100%;\n  }\n}\n\n.icons {\n  display: flex;\n  flex-wrap: wrap;\n  background: #fff;\n  &__item {\n    width: 20%;\n    margin-top: 0.1rem;\n  }\n  &__item__img {\n    display: block;\n    width: 0.4rem;\n    height: 0.4rem;\n    margin: 0 auto;\n    border-radius: 0.3rem;\n  }\n  &__item__desc {\n    text-align: center;\n    margin: 0.16rem 0 0.16rem 0;\n  }\n}\n\n.gap {\n  height: 0.1rem;\n  background: #b7b7b7;\n}\n</style>\n"],"sourceRoot":""}]}