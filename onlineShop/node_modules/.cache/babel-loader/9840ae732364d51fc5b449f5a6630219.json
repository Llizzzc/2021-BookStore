;;AACA,IAAM,eAAc,GAAI,SAAlB,eAAkB,CAAC,SAAD,EAAe;AACrC,MAAM,MAAK,GAAI,SAAS,EAAxB;;AACA,MAAM,YAAW,GAAI,SAAf,YAAe,GAAM;AACzB,IAAA,YAAY,CAAC,UAAb,CAAwB,SAAxB;AACA,IAAA,YAAY,CAAC,UAAb,CAAwB,UAAxB;AACA,IAAA,SAAS,CAAC,OAAD,CAAT;AACA,IAAA,UAAU,CAAC,YAAM;AACf,MAAA,MAAM,CAAC,OAAP,CAAe;AAAE,QAAA,IAAI,EAAE;AAAR,OAAf;AACD,KAFS,EAEP,IAFO,CAAV;AAGF,GAPA;;AAQA,SAAO,YAAP;AACF,CAXA,C,CAaA;;;AACA,IAAM,mBAAkB,GAAI,SAAtB,mBAAsB,GAAM;AAChC,MAAM,MAAK,GAAI,SAAS,EAAxB;;AACA,MAAM,kBAAiB,GAAI,SAArB,kBAAqB,GAAM;AAC/B,IAAA,MAAM,CAAC,IAAP,CAAY;AAAE,MAAA,IAAI,EAAE;AAAR,KAAZ;AACF,GAFA;;AAGA,SAAO,kBAAP;AACF,CANA,C,CAQA;;;AACA,IAAM,YAAW,GAAI,SAAf,YAAe,GAAM;AACzB,MAAM,MAAK,GAAI,SAAS,EAAxB;;AACA,MAAM,cAAa,GAAI,SAAjB,cAAiB,GAAM;AAC3B,IAAA,MAAM,CAAC,IAAP,CAAY;AAAE,MAAA,IAAI,EAAE;AAAR,KAAZ;AACF,GAFA;;AAGA,SAAO,cAAP;AACF,CANA,C,CAQA;;;AACA,IAAM,iBAAgB,GAAI,SAApB,iBAAoB,GAAM;AAC9B,MAAM,MAAK,GAAI,SAAS,EAAxB;;AACA,MAAM,mBAAkB,GAAI,SAAtB,mBAAsB,GAAM;AAChC,IAAA,MAAM,CAAC,IAAP,CAAY;AAAE,MAAA,IAAI,EAAE;AAAR,KAAZ;AACF,GAFA;;AAGA,SAAO,mBAAP;AACF,CANA;;AAQA,eAAe;AACb,EAAA,IAAI,EAAE,cADO;AAEb,EAAA,UAAU,EAAE;AAAE,IAAA,MAAM,EAAN,MAAF;AAAU,IAAA,KAAI,EAAJ;AAAV,GAFC;AAGb,EAAA,KAHa,mBAGJ;AAAA,0BACmC,cAAc,EADjD;AAAA,QACC,IADD,mBACC,IADD;AAAA,QACO,YADP,mBACO,YADP;AAAA,QACqB,SADrB,mBACqB,SADrB;;AAEP,QAAM,YAAW,GAAI,eAAe,CAAC,SAAD,CAApC;AACA,QAAM,kBAAiB,GAAI,mBAAmB,EAA9C;AACA,QAAM,QAAO,GAAI,aAAa,EAA9B;AACA,QAAM,cAAa,GAAI,YAAY,EAAnC;AACA,QAAM,mBAAkB,GAAI,iBAAiB,EAA7C;AACA,WAAO;AAAE,MAAA,QAAQ,EAAR,QAAF;AAAY,MAAA,YAAY,EAAZ,YAAZ;AAA0B,MAAA,kBAAkB,EAAlB,kBAA1B;AAA8C,MAAA,IAAI,EAAJ,IAA9C;AAAoD,MAAA,YAAY,EAAZ,YAApD;AAAkE,MAAA,cAAc,EAAd,cAAlE;AAAkF,MAAA,mBAAkB,EAAlB;AAAlF,KAAP;AACF;AAXa,CAAf","sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <div class=\"header\">\n      <div class=\"header__info\">\n        <div class=\"header__info__user\">用户名: {{ userInfo.name }}</div>\n        <div class=\"header__info__id\">ID: {{ userInfo.id }}</div>\n      </div>\n      <img\n        class=\"header__avatar\"\n        src=\"../../img/others/4.jpeg\"\n      />\n    </div>\n    <div class=\"list\">\n      <div class=\"list__item\">\n        <div class=\"list__item__icon iconfont\">&#xf05a;</div>\n        <div class=\"list__item__test\">我的地址</div>\n        <div\n          class=\"list__item__arrow iconfont\"\n          @click=\"handleAddressClick\"\n        >&#xe602;</div>\n      </div>\n      <div class=\"list__item\">\n        <div class=\"list__item__icon iconfont\">&#xe65a;</div>\n        <div class=\"list__item__test\">名字变更</div>\n        <div\n          class=\"list__item__arrow iconfont\"\n          @click=\"handleUsernameClick\"\n        >&#xe602;</div>\n      </div>\n      <div class=\"list__item\">\n        <div class=\"list__item__icon iconfont\">&#xe654;</div>\n        <div class=\"list__item__test\">修改密码</div>\n        <div\n          class=\"list__item__arrow iconfont\"\n          @click=\"handlePswClick\"\n        >&#xe602;</div>\n      </div>\n    </div>\n    <div\n      class=\"logout\"\n      @click=\"handleLogout\"\n    >退出登陆</div>\n  </div>\n  <Toast\n    v-if=\"show\"\n    :message=\"toastMessage\"\n  />\n  <Docker :currentIndex=\"3\" />\n</template>\n\n<script>\nimport { useRouter } from 'vue-router'\nimport Docker from '../../components/Docker'\nimport Toast, { useToastEffect } from '../../components/Toast'\n\n// 获取用户信息\nconst useUserEffect = () => {\n  const userInfo = JSON.parse(localStorage.getItem('user'))\n  return userInfo\n}\n\n// 用户退出\nconst useLogOutEffect = (showToast) => {\n  const router = useRouter()\n  const handleLogout = () => {\n    localStorage.removeItem('isLogin')\n    localStorage.removeItem('cartList')\n    showToast('注销成功!')\n    setTimeout(() => {\n      router.replace({ name: 'Login' })\n    }, 2000)\n  }\n  return handleLogout\n}\n\n// 获取地址跳转\nconst useGetAddressEffect = () => {\n  const router = useRouter()\n  const handleAddressClick = () => {\n    router.push({ name: 'MyAddressList' })\n  }\n  return handleAddressClick\n}\n\n// 改密跳转\nconst usePswEffect = () => {\n  const router = useRouter()\n  const handlePswClick = () => {\n    router.push({ name: 'PswChange' })\n  }\n  return handlePswClick\n}\n\n// 改名跳转\nconst useUsernameEffect = () => {\n  const router = useRouter()\n  const handleUsernameClick = () => {\n    router.push({ name: 'UsernameChange' })\n  }\n  return handleUsernameClick\n}\n\nexport default {\n  name: 'PersonalInfo',\n  components: { Docker, Toast },\n  setup () {\n    const { show, toastMessage, showToast } = useToastEffect()\n    const handleLogout = useLogOutEffect(showToast)\n    const handleAddressClick = useGetAddressEffect()\n    const userInfo = useUserEffect()\n    const handlePswClick = usePswEffect()\n    const handleUsernameClick = useUsernameEffect()\n    return { userInfo, handleLogout, handleAddressClick, show, toastMessage, handlePswClick, handleUsernameClick }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.wrapper {\n  overflow-y: auto;\n  position: absolute;\n  left: 0;\n  top: 0;\n  bottom: 0.5rem;\n  right: 0;\n  background: #f8f8f8;\n}\n.header {\n  position: relative;\n  height: 1.5rem;\n  background-image: linear-gradient(239deg, #3a6ff3 0%, #50c7fb 100%);\n  padding-top: 0.4rem;\n  &__info {\n    margin: 0.3rem 0.18rem 0 0.18rem;\n    padding-top: 0.46rem;\n    height: 0.66rem;\n    background: #fff;\n    border-radius: 0.08rem;\n    &__user {\n      font-size: 0.24rem;\n      color: #262628;\n      text-align: center;\n      line-height: 0.36rem;\n    }\n    &__id {\n      margin-top: 0.04rem;\n      font-size: 0.14rem;\n      color: #c1c0c9;\n      text-align: center;\n    }\n  }\n  &__avatar {\n    position: absolute;\n    left: 50%;\n    top: 0.2rem;\n    transform: translateX(-50%);\n    width: 0.94rem;\n    height: 0.94rem;\n    border-radius: 50%;\n  }\n}\n.list {\n  margin: 0.16rem 0.18rem 0 0.18rem;\n  background: #f8f8f8;\n  box-shadow: 0 0.08rem 0.16rem 0 rgba(0, 0, 0, 0.08);\n  border-radius: 0.08rem;\n  &__item {\n    display: flex;\n    padding: 0.2rem;\n    line-height: 0.22rem;\n    font-size: 0.14rem;\n    color: #262626;\n    &__icon {\n      margin-right: 0.12rem;\n      width: 0.22rem;\n      height: 0.22rem;\n      background: #32c5ff;\n      border-radius: 0.08rem;\n      text-align: center;\n      font-weight: bold;\n      color: #f8f8f8;\n    }\n    &__test {\n      flex: 1;\n    }\n    &__arrow {\n      width: 0.2rem;\n      transform: rotate(180deg);\n      font-weight: bold;\n      color: #c2c4ca;\n    }\n  }\n}\n.logout {\n  margin: 0.18rem;\n  line-height: 0.32rem;\n  background: #0091ff;\n  color: #f8f8f8;\n  text-align: center;\n  border-radius: 0.04rem;\n}\n</style>\n"],"sourceRoot":""}]}